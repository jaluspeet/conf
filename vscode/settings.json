{
    "workbench.colorCustomizations": {
        "[Default Dark Modern]": {
            "widget.shadow": "#0000",
            "scrollbar.shadow": "#0000",
            "inlineChat.shadow": "#0000",
            "listFilterWidget.shadow": "#0000",
            "diffEditor.unchangedRegionShadow": "#0000",
            "statusBarItem.remoteBackground": "#0000",
            "tab.activeBorderTop": "#0000",
            "editor.lineHighlightBackground": "#161616",
        },
        "[Default Light Modern]": {
            "scrollbar.shadow": "#0000",
            "tab.activeBorderTop": "#0000",
            "widget.shadow": "#0000",
            "sideBarSectionHeader.background": "#00000011",
            "editor.lineHighlightBackground": "#00000011",
            "editorGroupHeader.tabsBackground": "#bbbbbb",
            "panel.background": "#bbbbbb",
            "sideBar.background": "#bbbbbb",
            "statusBar.background": "#bbbbbb",
            "tab.inactiveBackground": "#bbbbbb",
            "editorHoverWidget.background": "#bbbbbb",
            "tab.activeBorder": "#dddddd",
            "editor.background": "#dddddd",
            "tab.activeBackground": "#dddddd",
        }
    },
    "extensions.experimental.affinity": {
        "vscodevim.vim": 1
    },
    "editor.fontSize": 12,
    "terminal.integrated.fontSize": 12,
    "window.autoDetectColorScheme": true,
    "editor.minimap.enabled": false,
    "security.workspace.trust.enabled": false,
    "explorer.confirmDelete": false,
    "debug.toolBarLocation": "docked",
    "workbench.sideBar.location": "right",
    "files.trimTrailingWhitespace": true,
    "git.enableSmartCommit": true,
    "git.autofetch": true,
    "breadcrumbs.enabled": false,
    "editor.fontLigatures": true,
    "workbench.editor.enablePreview": false,
    "comments.openView": "never",
    "explorer.autoReveal": false,
    "debug.onTaskErrors": "showErrors",
    "debug.internalConsoleOptions": "neverOpen",
    "explorer.confirmDragAndDrop": false,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.tips.enabled": false,
    "editor.showFoldingControls": "always",
    "extensions.ignoreRecommendations": true,
    "git.openRepositoryInParentFolders": "always",
    "git.confirmSync": false,
    "telemetry.telemetryLevel": "off",
    "editor.renderLineHighlight": "all",
    "explorer.compactFolders": false,
    "terminal.integrated.shellIntegration.decorationsEnabled": "never",
    "window.density.editorTabHeight": "compact",
    "workbench.tree.renderIndentGuides": "always",
    "editor.scrollbar.horizontal": "hidden",
    "terminal.integrated.tabs.enabled": false,
    "workbench.activityBar.location": "top",
    "editor.lineNumbers": "relative",
    "explorer.confirmPasteNative": false,
    "window.commandCenter": false,
    "workbench.layoutControl.enabled": false,
    "editor.accessibilitySupport": "off",
    "window.openFoldersInNewWindow": "off",
    "editor.cursorStyle": "block",
    "window.titleBarStyle": "native",
    "window.customTitleBarVisibility": "never",
    "terminal.integrated.persistentSessionReviveProcess": "never",
    "workbench.reduceMotion": "on",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "update.showReleaseNotes": false,
    "workbench.panel.showLabels": false,
    "accessibility.verbosity.panelChat": false,
    "task.problemMatchers.neverPrompt": true,
    "task.allowAutomaticTasks": "off",
    "task.autoDetect": "off"
}